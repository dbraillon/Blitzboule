<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />

    <title>@ViewBag.Title</title>

    @Styles.Render("~/Content/themes/bootstrap/bootstrap-theme.min.css")
    @Styles.Render("~/Content/themes/bootstrap/bootstrap.min.css")

    @Scripts.Render("~/bundles/modernizr")
</head>
<body>

    <div class="container">
        @RenderBody()
    </div>
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bootstrap.min.js")
    @RenderSection("scripts", required: false)
</body>
</html>
